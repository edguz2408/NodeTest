        .row
          form(action="#")
            .large-3.columns
              h3 Newsletter Signup
            .small-4.large-3.columns
              input.input.field.primary(type="text", placeholder="Enter Your Name")
            .small-5.large-4.columns
              input.input.field.primary(type="text", placeholder="Enter Your E-mail")
            .small-3.large-2.columns
              input.input.button.primary.red(type="submit", value="Subscribe")
