              li
                a(href="coupons.html") Coupons
              li
                a(href="post.html") Post
          .large-3.columns
            .account.cf
              a.input.button.blue.tertiary.icon.plus(href="#") Join us
              a.input.button.transparent.tertiary(href="#") Log in
      h1.text-center Search Results
      .search-wrap.stripe-white
        .row
          form(action="/results/", method="get")
            .small-6.large-8.columns
              label.search-box
                input#inputSearch.input.field.primary(type="text", name="searchInput" placeholder="Search listing")
                a#clear-sb.icon.clear.ir(href="#") Clear
            .small-3.large-2.columns
              input#btnSearch.input.button.primary.red(type="submit", value="Search")
            nav.small-3.large-2.columns
              ul.rr.menu-browse
                li.input.button.primary
                  p.label Browse
                  ul.rr.sub
                    li
                      a(href="company.html") Nike
