extends layout

block content
  .page.search-results
    header.cf(role="banner")
      .top-wrap
        .row
          .large-3.columns
            .logo
              a(href="index.html") Offers Finder
          nav.large-6.columns
            ul.rr.main-menu
              li
                a(href="index.html") Home
              li
                a(href="blog.html") Blog
              li.current
                a(href="search-results.html") Extra
